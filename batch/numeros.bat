cls
title Calculadora - Raizes cubicas
setlocal enabledelayedexpansion
echo Raiz cubicas
set count=0
set dend=%1
set num=%dend%
IF %dend% LEQ 0 (
CLS
echo PENDEJO IMBECIL, SOLO NUMEROS MAYORES A 0
PAUSE > NUL
GOTO raizes
)
if %dend% GTR 1290 (
cls
echo el maximo numero que esta calculadora le puede sacar raiz cubica a un numero es 1290.
pause > nul
goto raizes
)
for /l %%i in (%dend%, -1, 1) do (
   set /a cur=%%i*%%i*%%i
   if !cur! leq %dend% (
      set digit=%%i.
     set root=%%i
      goto out3
   )
)

:out3
   call set /a count=%%count%%+1
   if %count% GTR 5 goto next3
   set /a dend=(%dend%-%cur%)*100
   set /a div=%root%*2
   for /l %%i in (9,-1,0) do (
      set /a sqr=%div%%%i*%%i*%%i
      if !cur! leq %dend% (
         set root=%root%%%i
         goto out3
      )
   )
   
:next3
   set root=%root:1,6%
   if %dend% neq 0 set digit=%digit%%root%
   echo la raiz cubica de %num% es %digit%
pause > nul
goto raizes

